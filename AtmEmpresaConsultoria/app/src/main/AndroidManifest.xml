<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="atmempresaconsultoria.cursoandroid.com.atmempresaconsultoria" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".EmpresaActivity"
            android:label="@string/title_activity_empresa" >
        </activity>
        <activity
            android:name=".ServicoActivity"
            android:label="@string/title_activity_servico" >
        </activity>
        <activity
            android:name=".ClienteActivity"
            android:label="@string/title_activity_cliente" >
        </activity>
        <activity
            android:name=".ContatoActivity"
            android:label="@string/title_activity_contato" >
        </activity>
    </application>

</manifest>
